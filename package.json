{
  "name": "metna-mysql-express-typescript-node-api",
  "version": "1.0.0",
  "description": "METNA MySQL Express TypeScript Node API",
  "main": "app/server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec 'tsc && node ./build/server.js'",
    "start": "npm run build && npm run dev",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'app/**/*.ts'",
    "test": "npm run build && jest",
    "test:watch": "jest --watchAll",
    "deploy:test": "npm run build && npm run lint && npm run test"
  },
  "keywords": [
    "node.js",
    "jwt",
    "typescript",
    "jsonwebtoken",
    "jest",
    "authentication",
    "express",
    "mysql"
  ],
  "author": "samueleast",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.496.0",
    "@aws-sdk/s3-request-presigner": "^3.496.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "google-auth-library": "^9.5.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "prettier": "^3.2.4",
    "sequelize": "^6.32.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "aws-sdk-client-mock": "^3.0.1",
    "aws-sdk-client-mock-jest": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2"
  }
}
